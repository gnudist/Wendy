<script language="JavaScript">
document.getElementById( "jsWarning" ).innerHTML = "&nbsp;";

function displayInstallDialog( moduleName )
{
	var newInner = "";

	[DISPLAY_INSTALL_DIALOG_BODY]

	
	document.getElementById( "actionsDivModule" + moduleName ).innerHTML = newInner;
	return false;
}

function displayUninstallDialog( moduleName )
{
	var newInner = "";

	[DISPLAY_UNINSTALL_DIALOG_BODY]

	document.getElementById( "actionsDivModule" + moduleName ).innerHTML = newInner;

	return false;
}

function resetModuleActions( moduleName )
{
	var newInner = "";

	[RESET_MODULE_ACTIONS_BODY]

	document.getElementById( "actionsDivModule" + moduleName ).innerHTML = newInner;
	return false;
}


</script>

